  {% endif %}
  
  {% if block.settings.show_arrows %}
    <button class="pc-brand-slider__arrow pc-brand-slider__arrow--prev">←</button>
  {% endif %}
  
  <div class="pc-brand-slider__slider" id="brand-slider-{{ block.id }}">
    {% for i in (1..6) %}
      {% assign image_key = 'brand_' | append: i | append: '_image' %}
      {% if block.settings[image_key] != blank %}
        <div class="pc-brand-slider__item">
          {{ block.settings[image_key] | image_url: width: 300 | image_tag: loading: 'lazy', alt: 'Brand logo' }}
        </div>
      {% else %}
        <div class="pc-brand-slider__item">
          {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
        </div>
      {% endif %}
    {% endfor %}
  </div>
  
  {% if block.settings.show_arrows %}
    <button class="pc-brand-slider__arrow pc-brand-slider__arrow--next">→</button>
  {% endif %}
  
  {% if block.settings.show_dots %}
    <div class="pc-brand-slider__dots">
      {% for i in (1..6) %}
        <div class="pc-brand-slider__dot {% if forloop.first %}active{% endif %}" data-index="{{ forloop.index0 }}"></div>
      {% endfor %}
    </div>
  {% endif %}
</div>

<script>
document.addEventListener('DOMContentLoaded', function() {
  // Brand slider functionality
  const brandSliders = document.querySelectorAll('.pc-brand-slider__slider');
  
  brandSliders.forEach(slider => {
    const wrapper = slider.closest('.pc-brand-slider');
    const prevBtn = wrapper.querySelector('.pc-brand-slider__arrow--prev');
    const nextBtn = wrapper.querySelector('.pc-brand-slider__arrow--next');
    const dots = wrapper.querySelectorAll('.pc-brand-slider__dot');
    
    if (prevBtn) {
      prevBtn.addEventListener('click', () => {
        slider.scrollBy({
          left: -slider.clientWidth,
          behavior: 'smooth'
        });
      });
    }
    
    if (nextBtn) {
      nextBtn.addEventListener('click', () => {
        slider.scrollBy({
          left: slider.clientWidth,
          behavior: 'smooth'
        });
      });
    }
    
    if (dots.length > 0) {
      dots.forEach(dot => {
        dot.addEventListener('click', () => {
          const index = parseInt(dot.dataset.index);
          slider.scrollTo({
            left: slider.clientWidth * index,
            behavior: 'smooth'
          });
        });
      });
      
      slider.addEventListener('scroll', () => {
        const activeIndex = Math.round(slider.scrollLeft / slider.clientWidth);
        dots.forEach((dot, index) => {
          dot.classList.toggle('active', index === activeIndex);
        });
      });
    }
  });
});
</script>

