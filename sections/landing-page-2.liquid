<script>
class PCTeam extends HTMLElement {
  constructor() {
    super();
    this.sliderEnabled = this.classList.contains('pc-slider-mode');
    this.grid = this.querySelector('.pc-team__grid');
    this.prevBtn = this.querySelector('.pc-team__arrow--prev');
    this.nextBtn = this.querySelector('.pc-team__arrow--next');
    this.dotsContainer = this.querySelector('.pc-team__dots');
    this.dots = this.dotsContainer ? this.dotsContainer.querySelectorAll('.pc-team__dot') : [];
    this.members = this.querySelectorAll('.pc-team-member');
    
    this.initComponent();
  }

  initComponent() {
    if (!this.sliderEnabled) return;
    
    // Check if we need slider behavior
    this.checkSliderNeeded();
    
    // Initialize only if needed and on mobile
    if (this.needsSlider && window.innerWidth <= 768) {
      this.initSlider();
    }
    
    // Add resize listener
    window.addEventListener('resize', () => this.handleResize());
  }

  checkSliderNeeded() {
    this.needsSlider = this.members.length > 1;
    
    // Hide navigation if not needed
    if (!this.needsSlider) {
      if (this.prevBtn) this.prevBtn.style.display = 'none';
      if (this.nextBtn) this.nextBtn.style.display = 'none';
      if (this.dotsContainer) this.dotsContainer.style.display = 'none';
    }
  }

  initSlider() {
    // Set up navigation
    if (this.prevBtn) {
      this.prevBtn.style.display = 'flex';
      this.prevBtn.addEventListener('click', () => this.prevSlide());
    }
    
    if (this.nextBtn) {
      this.nextBtn.style.display = 'flex';
      this.nextBtn.addEventListener('click', () => this.nextSlide());
    }
    
    // Set up dots
    if (this.dots.length > 0) {
      this.dotsContainer.style.display = 'flex';
      this.dots.forEach(dot => {
        dot.addEventListener('click', () => this.goToSlide(parseInt(dot.dataset.index)));
      });
      
      // Update dots on scroll
      this.grid.addEventListener('scroll', () => this.updateDots());
    }
  }

  prevSlide() {
    this.grid.scrollBy({
      left: -this.grid.clientWidth,
      behavior: 'smooth'
    });
  }

  nextSlide() {
    this.grid.scrollBy({
      left: this.grid.clientWidth,
      behavior: 'smooth'
    });
  }

  goToSlide(index) {
    this.grid.scrollTo({
      left: this.grid.clientWidth * index,
      behavior: 'smooth'
    });
  }

  updateDots() {
    const activeIndex = Math.round(this.grid.scrollLeft / this.grid.clientWidth);
    this.dots.forEach((dot, index) => {
      dot.classList.toggle('active', index === activeIndex);
    });
  }

  handleResize() {
    if (window.innerWidth <= 768 && this.needsSlider) {
      if (!this.sliderInitialized) {
        this.initSlider();
        this.sliderInitialized = true;
      }
    } else {
      // Reset to grid layout on desktop
      if (this.prevBtn) this.prevBtn.style.display = 'none';
      if (this.nextBtn) this.nextBtn.style.display = 'none';
      if (this.dotsContainer) this.dotsContainer.style.display = 'none';
      this.sliderInitialized = false;
    }
  }

  disconnectedCallback() {
    window.removeEventListener('resize', () => this.handleResize());
  }
}

customElements.define('pc-team', PCTeam);
</script>
